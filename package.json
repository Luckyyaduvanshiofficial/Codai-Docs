  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=\"--max-old-space-size=8192\" next dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --max-semi-space-size=128\" next build",
    "start": "next start",
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); ['.next', '.turbo', 'node_modules/.cache'].forEach(dir => { try { fs.rmSync(dir, { recursive: true, force: true }); console.log('Cleaned:', dir); } catch(e) {} });\""
  },
  "dependencies": {
    "next": "^16.0.0",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
